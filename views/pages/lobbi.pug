extends /layouts/two_column_3_9

block head
  link(href="/css/lobbi/chat.css?v=1.3" rel="stylesheet")
  link(href="/css/lobbi/game.css?v=1.2" rel="stylesheet")
  link(href="/css/lobbi/form.css" rel="stylesheet")

block main
  include /includes/lobbi/loader

  include /includes/lobbi/chat
  include /includes/lobbi/games

  if currentAccount 
    include /includes/lobbi/claim-form

block aside
  include /includes/lobbi/friends_online
  include /includes/lobbi/top
  include /includes/lobbi/online_form

block scripts 
  include /includes/lobbi/lobbi-tmpl
  include /includes/online-tmpl
  script(src="/js/lobbi/base.js?v=1.1")
  script(src="/js/lobbi/lobbi.js?v=1.8")
  script(src="/js/lobbi/game.js?v=1.8")
  script(src="/js/lobbi/online.js?v=1.2")
  if currentAccount && currentAccount.role == 1
    script(src="/js/admin/lobbi/moder.js")
    script(src="/js/admin/lobbi/timeset.js")

